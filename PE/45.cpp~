#include <iostream>
#include <cmath>
using namespace std;

bool isPentagonal(int n){
    int x=(1+sqrt(1+24*n))/6;
    if(x*(3*x-1)/2 == n) return true;
    return false;
}

bool isTriangle(int n){
    int x=(-1+sqrt(1+8*n))/2;
    if(x*(x+1)/2 == n) return true;
    return false;
}

bool isHexagonal(int n){
    int x=(1+sqrt(1+8*n))/4;
    if(x*(2*x-1) == n) return true;
    return false;
}

int main(){
    int pi,pj;
    int count=0;
    for(int i=1;count<2;i++){
        if(isTriangle(i*(2*i-1)) && isPentagonal(i*(2*i-1))){
            cout << i*(2*i-1) << '\n';
            count++;
        }
    }
    return 0;
}
