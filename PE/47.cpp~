#include <iostream>
using namespace std;

bool data[1000000];

bool isPrime(int n){
    return data[n];
}

int mcount(int n){
    int ret=0;
    if(isPrime(n)){
        return 1;
    }
    for(int i=2;i<=n;i++){
        if(n%i == 0){
            if(isPrime(i)){
                ret++;
            }
        }
    }
    return ret;
}

int main(){
    for(int i=2;i<1000000;i++){
        data[i] = true;
    }
    for(int i=2;i<1000000;i++){
        if(data[i]){
            for(int j=2;i*j<1000000j++){
                data[i*j] = false;
            }
        }
    }
    for(int i=3;;i++){
        if(mcount(i) == mcount(i+1)){
            if(mcount(i+1) == mcount(i+2) && mcount(i+2) == mcount(i+3) && mcount(i) == 4){
                cout << i << '\n';
                break;
            }
        }
    }
    return 0;
}

