#include <iostream>
using namespace std;

bool isPentagonal(int n){
    for(int i=1;i*(i*3-1)>2*n){
        if(i*(i*3-1) == 2*n){
            return true;
        }
    }
    return false;
}

int main(){
    int pi,pj;
    for(int i=2;;i++){
        pi = i*(3*i-1)/2;
        for(int j=i-1;j>0;j--){
            pj = j*(3*j-1)/2;
            if(isPentagonal(pi + pj) && isPentagonal(pi - pj)){
                cout << (pi - pj) << '\n';
                return 0;
            }
        }
    }
    return 0;
}
