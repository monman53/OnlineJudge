#include <iostream>
using namespace std;

long long int dp[1001][21];
int data[100];
int n;

long long int rec(int i,int s){
    if(dp[i][s] >= 0){
        return dp[i][s];
    }
    long long int res=0;
    if(i == n-2){
        if(data[i] == s){
            res = 1;
        }else{
            res = 0;
        }
    }else{
        if(s-data[i] >=0 && s-data[i] <= 20){
            res += rec(i+1, s-data[i]);
        }
        if(data[i]-s >=0 && data[i]-s <= 20){
            res += rec(i+1, data[i]-s);
        }
    }
    return dp[i][s] = res;
}

int main(){
    int n;
    for(int i=0;i<n;i++){
        for(int j=0;i<21;j++){
            dp[i][j] = -1;
        }
    }
    cin >> n;
    for(int i=0;i<n;i++){
        cin >> data[i];
    }
    cout << rec(0,data[n-1]) << '\n';
    return 0;
}
