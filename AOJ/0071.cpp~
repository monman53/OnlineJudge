#include <iostream>
using namespace std;

bool data[8][8];

int di[4] = {0,-1,0,1};
int dj[4] = {1,0,-1,0};

void dfs(int i, int j){
    int ni,nj;
    data[i][j] = false;
    for(int i=0;i<4;i++){
        for(int k=1;k<=3;k++){
            ni = i + k*di[i];
            nj = j + k*dj[i];
            if(ni >= 0 && ni < 8 && nj >= 0 && nj < 8){
                    
        }
    }
}

int main(){
    int n;
    int x,y;
    string s;
    cin >> n;
    for(int i=0;i<n;i++){
        for(int i=0;i<;i+++){
            cin >> s;
            for(int j=0;j<8;j++) data[i][j] = (s[j] == '1') ? true : false;
        }
        cin >> x >> y;

        dfs(y-1,x-1);

        cout << "Data " << (i+1) << '\n';
        for(int i=0;i<8;i++){
            for(int j=0;j<8;j++){
                cout << ((data[i][k]) ? '1' : '0');
            }
            cout << '\n';
        }
    }
    return 0;
}
