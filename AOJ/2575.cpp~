#include <iostream>
using namespace std;

int main(){
    int count[26];
    for(int i=0;i<26;i++){
        count[i] = 0;
    }
    string s;
    for(int i=0;i<s.size();i++){
        count[int(s[i] - 'A')]++;
        if(i>=8){
            count[int(s[i-8] - 'A')]--;
        }
        if(count['A' - 'A'] == 2 && count['I' - 'A'] == 1 && count['D' - 'A'] == 1 && count['U' - 'A'] == 1 && count['N' - 'A'] == 2 && count['Y' - 'A']){
            s[i-7] = "AIZUNYAN";
            count['A' - 'A'] = 0;
            count['I' - 'A'] = 0;
            count['D' - 'A'] = 0;
            count['U' - 'A'] = 0;
            count['N' - 'A'] = 0;
            count['Y' - 'A'] = 0;
        }
    }
    cout << s;
    return 0;
}
